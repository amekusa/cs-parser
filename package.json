{
  "name": "cs-parser",
  "version": "1.3.0",
  "description": "Context-sensitive parser framework",
  "main": "./lib/index.js",
  "scripts": {
    "test": "c8 mocha --enable-source-maps",
    "test:examples": "node examples/",
    "test:watch": "npm test -- --watch",
    "codecov": "c8 report -r lcov && codecov",
    "docs": "jsdoc -c .jsdoc.json",
    "docs:clean": "rm -rf docs/cs-parser/$npm_package_version",
    "docs:publish": "git subtree push --prefix docs/cs-parser origin gh-pages",
    "build": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "prepublish": "npm run test && npm run docs && npm run build"
  },
  "files": [
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amekusa/cs-parser.git"
  },
  "keywords": [
    "parser",
    "context",
    "framework"
  ],
  "author": "Satoshi Soma (https://amekusa.com)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/amekusa/cs-parser/issues"
  },
  "homepage": "https://github.com/amekusa/cs-parser#readme",
  "devDependencies": {
    "develarms": "^2.1.0"
  },
  "develarms": {
    "c8": "^8.0.1",
    "mocha": "^10.2.0",
    "rollup": "^3.28.0",
    "jsdoc": "^3.6.11"
  }
}
