{
  "name": "cs-parser",
  "version": "1.3.0",
  "description": "Context-sensitive parser framework",
  "type": "module",
  "exports": {
    "import": "./dist/import/bundle.mjs",
    "require": "./dist/require/bundle.cjs"
  },
  "scripts": {
    "setup": "npm i && develarms i",
    "test": "c8 mocha --enable-source-maps",
    "codecov": "npm run test && c8 report --reporter=text-lcov > coverage.lcov && codecov",
    "docs": "jsdoc -c .jsdoc.json && cd docs/$npm_package_name && ln -sfn $npm_package_version latest",
    "docs:clean": "rm -rf docs/$npm_package_name/$npm_package_version && npm run docs",
    "docs:publish": "git subtree push --prefix docs/$npm_package_name origin gh-pages",
    "build": "rollup -c .rollup.js",
    "build:clean": "rm -rf dist && npm run build"
  },
  "files": [
    "LICENSE",
    "NOTICE",
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amekusa/cs-parser.git"
  },
  "keywords": [
    "parser",
    "context",
    "dsl",
    "language",
    "data",
    "format"
  ],
  "author": "Satoshi Soma (https://amekusa.com)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/amekusa/cs-parser/issues"
  },
  "homepage": "https://github.com/amekusa/cs-parser#readme",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.1",
    "develarms": "^2.1.0",
    "docolatte": "^3.5.1"
  },
  "develarms": {
    "c8": "^8.0.1",
    "mocha": "^10.2.0",
    "rollup": "^3.28.0",
    "jsdoc": "^3.6.11",
    "codecov": "^3.8.3"
  }
}
